<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
	<system.serviceModel>
		<services>
			<service name="MemoryGameService.MemoryGameService" behaviorConfiguration="mexBehavior">
				<endpoint address="MemoryGameService" binding="netTcpBinding" contract="MemoryGameService.IAccessibilityService">
				</endpoint>
				<endpoint address="MemoryGameService" binding="netTcpBinding" contract="MemoryGameService.ICommunicationService">
				</endpoint>
				<endpoint address="MemoryGameService" binding="netTcpBinding" contract="MemoryGameService.IMemoryGameService">
				</endpoint>
				<endpoint address="MemoryGameService" binding="netTcpBinding" contract="MemoryGameService.IMailingService">
				</endpoint>
				<host>
					<baseAddresses>
						<add baseAddress="http://localhost:8080"/>
						<add baseAddress="net.tcp://localhost:8090"/>
					</baseAddresses>
				</host>
			</service>
		</services>
		<behaviors>
			<serviceBehaviors>
				<behavior name="mexBehavior">
					<serviceMetadata httpGetEnabled="true"/>
				</behavior>
			</serviceBehaviors>
		</behaviors>
	</system.serviceModel>
</configuration>